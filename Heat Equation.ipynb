   ],
   "source": [
    "# initialization\n",
    "T = 1\n",
    "dx = 1/J   # step size of x axis\n",
    "dt = 1/N   # step size of t axis\n",
    "r = alpha * dt / (dx * dx)\n",
    "x = np.arange(J + 1) * dx\n",
    "t = np.arange(N + 1) * dt\n",
    "temp = np.zeros(J + 1) \n",
    "\n",
    "# which is the RHS of the equation\n",
    "for i in range(1,J):\n",
    "     temp[i] = math.sin(math.pi * i * dx)\n",
    "print(temp)\n",
    "\n",
    "# construct tradiagonal matrix\n",
    "upper = np.ones(J - 2) * (- r)\n",
    "main = np.ones(J - 1) * (1 + 2 * r)\n",
    "lower = np.ones(J - 2) * (-r)"
   ]
  },
   ],
   "source": [
    "# get the value of U\n",
    "U = np.zeros(shape = (J+1,len(t)))\n",
    "U[1:J,0] = TDMA(lower,main,upper,temp[1:J])\n",
    "for n in range(1,len(t)):\n",
    "    U[1:J,n] = TDMA(lower, main, upper, U[1:J,n-1])\n"
   ]
  },
