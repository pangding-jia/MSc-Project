   "source": [
    "# exact analyical solution\n",
    "U_exact = np.zeros(N+1) \n",
    "t = np.arange(N + 1) * dt\n",
    "for i in range(0,201):\n",
    "    U_exact[i] = math.exp(-t[i]*math.pi**2)*math.sin(math.pi*25*dx)"
   ]
